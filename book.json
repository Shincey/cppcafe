{
    "output": null,

    "generator": "site",

    "title": "CPP Cafe",
    "author": "Shincey Tao <shinceytao@gmail.com>",
    "description": null,

    "extension": null,

    "plugins": ["chapter-fold", "pageview-count", "code", "back-to-top-button",
                "-sharing", "sharing-plus", "hide-element", "insert-logo", "katex"],

    "pluginsConfig": {
        "fontSettings": {
            "theme": "night",
            "family": "sans",
            "size": 2
        },
        "sharing": {
          "douban": true,
          "facebook": true,
          "google": true,
          "weibo": true,
          "qq": true,
          "qzone": true,
          "twitter": true,
          "pocket": true,
          "all" : [
            "douban", "qq", "qzone", "facebook", "google", "instapaper",
            "twitter", "weibo", "messenger", "whatsapp"
          ]
        },
        "hide-element": {
          "elements":[".gitbook-link"]
        },
        "insert-logo": {
          "url": "https://i.loli.net/2019/12/06/4IiyV7hZSXRfTlg.png",
          "style": "background: none; max-height: 30px; min-height: 30px"
        }

    },

    "variables": {},

    "links": {
        "sidebar": {
            "Shincey's Github": "https://github.com/Shincey"
        }
    },


    "pdf": {
        "pageNumbers": false,

        "fontSize": 12,

        "paperSize": "a4",

        "margin": {
            "right": 62,
            "left": 62,
            "top": 36,
            "bottom": 36
        },

        "headerTemplate": null,

        "footerTemplate": null
    }
}
